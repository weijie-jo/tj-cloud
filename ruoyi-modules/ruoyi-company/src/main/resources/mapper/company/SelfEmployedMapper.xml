    <sql id="selectSelfEmployedVo">
        select self_id, self_key, place_code, tax_id, self_address, self_name, legal_person_name, id_card_num, password, status, remark, user_id, exp_status, maximum, register_time, industry_type, organizational_form, number_employees, contribution_amount, city, county, electronic_commerce, free_trade_zone, free_trade_area, property_right, industry, nature_business from self_employed
    </sql>

    <select id="selectSelfEmployedList" parameterType="com.ruoyi.company.domain.SelfEmployed" resultMap="SelfEmployedResult">
        <include refid="selectSelfEmployedVo"/>
        <where>  
            <if test="placeCode != null  and placeCode != ''"> and place_code like concat('%', #{placeCode}, '%')</if>
            <if test="legalPersonName != null  and legalPersonName != ''"> and legal_person_name like concat('%', #{legalPersonName}, '%')</if>
            <if test="userId != null "> and user_id = #{userId}</if>
        </where>
    </select>
    
    <select id="selectSelfEmployedBySelfId" parameterType="String" resultMap="SelfEmployedResult">
        <include refid="selectSelfEmployedVo"/>
        where self_id = #{selfId}
    </select>
        
