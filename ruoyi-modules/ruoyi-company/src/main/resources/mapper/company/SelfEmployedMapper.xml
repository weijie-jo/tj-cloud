    <sql id="selectSelfEmployedVo">
        select self_id, self_code, place_code, user_id, tax_id, self_address, self_name, legal_person_name, id_card_num, password, status, remark, place_name, username, exp_status, maximum, register_time, industry_type, industry_tax, organizational_form, number_employees, contribution_amount, city, county, electronic_commerce, free_trade_zone, free_trade_area, property_right, nature_business, account_type, account_type2, private_deposit_bank, private_account_number, public_deposit_bank_1, public_account_number_1, public_deposit_bank_2, public_account_number_2, account_name, public_deposit_bank_3, public_account_number_3, create_by, update_by, create_time, update_time, business_status, file_name1, file_name2,file_name3,file_name4,file_name5, info_status, tax_status, bank_status ,employed_remark from self_employed
    </sql>

    <select id="selectEmployedJoinReview" parameterType="com.ruoyi.company.domain.vo.SelfEmployedVo" resultType="com.ruoyi.company.domain.vo.SelfEmployedVo">
        SELECT *FROM self_employed t JOIN self_name_review A on t.self_code=A.self_code JOIN  self_application_info B  on t.self_code=B.self_code
        JOIN  self_legal_person C  on t.self_code=C.self_code
        <where>
            (t.end_status=0 OR DATE_FORMAT(end_time,'%Y-%m-%d')>=DATE_SUB(CURDATE(),INTERVAL 7 DAY)  )
            <if test="placeName != null  and placeName != ''"> and place_name like concat('%', #{placeName}, '%')</if>
            <if test="legalPersonName != null  and legalPersonName != ''"> and legal_person_name like concat('%', #{legalPersonName}, '%')</if>
            <if test="nameStatus != null  "> and A.name_status =#{nameStatus}</if>
            <if test="infoStatus != null  "> and t.info_status =#{infoStatus}</if>
            <if test="taxStatus != null  "> and t.tax_status = #{taxStatus}</if>
            <if test="bankStatus != null  "> and t.bank_status =#{bankStatus}</if>
            <if test="businessStatus != null  "> and t.business_status =#{businessStatus}</if>
        </where>
    </select>

    <select id="selectEmployedJoinEnd" parameterType="com.ruoyi.company.domain.vo.SelfEmployedVo" resultType="com.ruoyi.company.domain.vo.SelfEmployedVo">
        SELECT *FROM self_employed t JOIN self_name_review A on t.self_code=A.self_code
        <where>
