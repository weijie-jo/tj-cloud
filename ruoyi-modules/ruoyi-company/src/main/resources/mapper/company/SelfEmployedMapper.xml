    <sql id="selectSelfEmployedVo">
        select self_id, self_code, place_code, user_id, tax_id, self_address, self_name, legal_person_name, id_card_num,
               password, status, place_name, username, exp_status, maximum, register_time, industry_type,
               industry_tax, organizational_form, number_employees, contribution_amount, city, county, electronic_commerce,
               free_trade_zone, free_trade_area, property_right, nature_business, account_type, account_type2, private_deposit_bank,
               private_account_number, public_deposit_bank_1, public_account_number_1, public_deposit_bank_2, public_account_number_2,
               account_name, public_deposit_bank_3, public_account_number_3, create_by, update_by, create_time, update_time, business_status,
               file_name1, file_name2,file_name3,file_name4,file_name5, info_status, tax_status, bank_status ,
               business_term,is_active,remark_name,remark_info,file_name6,file_name7,realname_status ,
               special_self_fee, ordinary_self_fee, special_self_money, ordinary_self_money, is_self_tax, register_money, file_name8,
               ordinary_tax, ordinary_special_tax, ordinary_share, ordinary_share_money, special_share, special_share_money,
               is_ordinary_tax,is_special_tax,remark_register
                from self_employed
    </sql>
    <sql id="selectSelfEmployedJoin">
        select * from self_employed t
                JOIN self_name_review A on t.self_code=A.self_code
                JOIN  self_application_info B  on t.self_code=B.self_code
                JOIN  self_legal_person C  on t.self_code=C.self_code
                JOIN sys_dict_data D on C.eduation=D.dict_value
    </sql>
    <select id="selectEmployedJoinReview"  resultType="com.ruoyi.company.domain.vo.SelfEmployedVo">
        <include refid="selectSelfEmployedJoin"/>
        <trim prefix="where" prefixOverrides="and|or">
            D.dict_type="educational_level"
            <if test="selfEmployedVo.endStatus == 0  ">AND t.end_status =0 </if>
            <if test="selfEmployedVo.endStatus == 1  ">AND t.end_status =1 AND DATE_FORMAT(end_time,'%Y-%m-%d')>=DATE_SUB(CURDATE(),INTERVAL 7 DAY)</if>
            <if test="selfEmployedVo.endStatus == 2  ">AND t.end_status =2 </if>
            <if test="selfEmployedVo.placeName != null  and selfEmployedVo.placeName != ''"> and t.place_name like concat('%', #{selfEmployedVo.placeName}, '%')</if>
            <if test="selfEmployedVo.legalPersonName != null  and selfEmployedVo.legalPersonName != ''"> and t.legal_person_name like concat('%', #{selfEmployedVo.legalPersonName}, '%')</if>
            <if test="selfEmployedVo.username != null  and selfEmployedVo.username != ''"> and t.username like concat('%', #{selfEmployedVo.username}, '%')</if>
            <if test="userIdArr != null  and userIdArr.size() >0 ">
                AND t.user_id in
                <foreach collection="userIdArr" index="index" item="item" open="(" close=")" separator=",">
                    #{item}
                </foreach>
            </if>
            <if test="selfEmployedVo.nameStatus != null  "> and A.name_status =#{selfEmployedVo.nameStatus}</if>
            <if test="selfEmployedVo.infoStatus != null  "> and t.info_status =#{selfEmployedVo.infoStatus}</if>
            <if test="selfEmployedVo.taxStatus != null  "> and t.tax_status = #{selfEmployedVo.taxStatus}</if>
            <if test="selfEmployedVo.bankStatus != null  "> and t.bank_status =#{selfEmployedVo.bankStatus}</if>
            <if test="selfEmployedVo.businessStatus != null  "> and t.business_status =#{selfEmployedVo.businessStatus}</if>
            <if test="selfEmployedVo.realnameStatus != null  "> and t.realname_status =#{selfEmployedVo.realnameStatus}</if>
            <if test="selfEmployedVo.selfCode != null  "> and t.self_code =#{selfEmployedVo.selfCode}</if>
            <if test="selfEmployedVo.start != null and selfEmployedVo.start != ''"><!-- 开始时间检索 -->
                AND date_format(t.create_time,'%Y-%m-%d %H:%i:%s') >= date_format(#{selfEmployedVo.start},'%Y-%m-%d %H:%i:%s')
            </if>
            <if test="selfEmployedVo.end != null and selfEmployedVo.end != ''"><!-- 结束时间检索 -->
                AND date_format(t.create_time,'%Y-%m-%d %H:%i:%s') &lt;= date_format(#{selfEmployedVo.end},'%Y-%m-%d %H:%i:%s')
            </if>
            <if test="selfEmployedVo.ordinaryTax != null "> and t.ordinary_tax = #{selfEmployedVo.ordinaryTax}</if>
            <if test="selfEmployedVo.ordinarySpecialTax != null "> and t.ordinary_special_tax = #{selfEmployedVo.ordinarySpecialTax}</if>
            <if test="selfEmployedVo.ordinaryShare != null "> and t.ordinary_share = #{selfEmployedVo.ordinaryShare}</if>
            <if test="selfEmployedVo.ordinaryShareMoney != null "> and t.ordinary_share_money = #{selfEmployedVo.ordinaryShareMoney}</if>
            <if test="selfEmployedVo.specialShare != null "> and t.special_share = #{selfEmployedVo.specialShare}</if>
            <if test="selfEmployedVo.specialShareMoney != null "> and t.special_share_money = #{selfEmployedVo.specialShareMoney}</if>
            <if test="selfEmployedVo.isOrdinaryTax != null "> and t.is_ordinary_tax = #{selfEmployedVo.isOrdinaryTax}</if>
            <if test="selfEmployedVo.isSpecialTax != null "> and t.is_special_tax = #{selfEmployedVo.isSpecialTax}</if>
            <if test="selfEmployedVo.remarkRegister != null  and selfEmployedVo.remarkRegister != ''"> and t.remark_register = #{selfEmployedVo.remarkRegister}</if>
        </trim>
        ORDER BY
        t.create_time DESC
    </select>

    <select id="selectEmployedJoinEnd"  resultType="com.ruoyi.company.domain.vo.SelfEmployedVo">
        <include refid="selectSelfEmployedJoin"/>
        <trim prefix="where" prefixOverrides="and|or">
